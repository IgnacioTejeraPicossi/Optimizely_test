{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "EPiServer": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "OptiDemoCms": "Debug"
      }
    }
  },
  "ConnectionStrings": {
    "EPiServerDB": "Data Source=(LocalDb)\\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\\OptiDemoCms.mdf;Initial Catalog=OptiDemoCms;Integrated Security=True;Connect Timeout=30"
  },
  "EPiServer": {
    "Cms": {
      "MappedRoles": {
        "Items": {
          "CmsEditors": { "MappedRoles": [ "WebEditors", "WebAdmins" ] },
          "CmsAdmins": { "MappedRoles": [ "WebAdmins" ] }
        }
      }
    }
  }
}
